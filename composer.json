{
    "name": "parkourben99/laravel-mysql-s3-backup",
    "description": "Backup the MySQL database of your Laravel app to Amazon S3",
    "authors": [
        {
            "name": "Trevor Fitzgerald",
            "email": "fitztrev@gmail.com"
        },
        {
            "name": "Benjamin Ayles",
            "email": "ben@aylessoftware.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.2||^6.0||^7.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "LaravelMysqlS3Backup\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMysqlS3Backup\\ServiceProvider"
            ]
        }
    }
}
